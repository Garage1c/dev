		<field xsi:type="DataSetFieldField">
			<dataPath>Комментарий</dataPath>
			<field>Комментарий</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Комментарий</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Разм.Заказ,
	Разм.Заказ.Ответственный КАК Ответственный,
	Разм.Заказ.ЗакупитьНедостающее КАК ЗакупитьНедостающее,
	Разм.Заказ.ДатаОтправкиВЗакупку КАК ДатаОтправкиВЗакупку,
	Разм.Заказ.Контрагент КАК Контрагент,
	Разм.Номенклатура.Производитель КАК Производитель,
	Разм.Номенклатура.КатегорияТовара КАК КатегорияТовара,
	Разм.Номенклатура,
	Разм.Очередь,
	Разм.КоличествоОстаток,
	ОстаткиНаСкладах.ОстатокВсего,
	ОстаткиНаСкладах.ОстатокНаЦСкладе,
	ТоварыВРезерве.Резерв,
	ТоварыВРезерве.РезервНаЦСкладе,
	ТоварыВПути.ВПути,
	Разм.Номенклатура.Поставщик.МенеджерЗакупок КАК МенеджерЗакупок,
	ВЫРАЗИТЬ(Разм.Номенклатура.Комментарий КАК СТРОКА(200)) КАК Комментарий
ПОМЕСТИТЬ Резервы
ИЗ
	РегистрНакопления.РазмещениеЗаказов.Остатки(, {(Номенклатура = &amp;Номенклатура)}) КАК Разм
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ТоварыНаСкладахОстатки.Номенклатура КАК Номенклатура,
			СУММА(ТоварыНаСкладахОстатки.КоличествоОстаток) КАК ОстатокВсего,
			СУММА(ВЫБОР
					КОГДА ТоварыНаСкладахОстатки.Склад В (&amp;Склад)
						ТОГДА ЕСТЬNULL(ТоварыНаСкладахОстатки.КоличествоОстаток, 0)
					ИНАЧЕ 0
				КОНЕЦ) КАК ОстатокНаЦСкладе
		ИЗ
			РегистрНакопления.ТоварыНаСкладах.Остатки КАК ТоварыНаСкладахОстатки
		
		СГРУППИРОВАТЬ ПО
			ТоварыНаСкладахОстатки.Номенклатура) КАК ОстаткиНаСкладах
		ПО Разм.Номенклатура = ОстаткиНаСкладах.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			СУММА(ТоварыВРезервеОстатки.КоличествоОстаток) КАК Резерв,
			ТоварыВРезервеОстатки.Номенклатура КАК Номенклатура,
			СУММА(ВЫБОР
					КОГДА ТоварыВРезервеОстатки.Размещение В (&amp;Склад)
						ТОГДА ЕСТЬNULL(ТоварыВРезервеОстатки.КоличествоОстаток, 0)
					ИНАЧЕ 0
				КОНЕЦ) КАК РезервНаЦСкладе
		ИЗ
			РегистрНакопления.ТоварыВРезерве.Остатки КАК ТоварыВРезервеОстатки
		
		СГРУППИРОВАТЬ ПО
			ТоварыВРезервеОстатки.Номенклатура) КАК ТоварыВРезерве
		ПО Разм.Номенклатура = ТоварыВРезерве.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ТоварыПоставщиковВПутиОстатки.Номенклатура КАК Номенклатура,
			ТоварыПоставщиковВПутиОстатки.КоличествоОстаток КАК ВПути
		ИЗ
			РегистрНакопления.ТоварыПоставщиковВПути.Остатки КАК ТоварыПоставщиковВПутиОстатки) КАК ТоварыВПути
		ПО Разм.Номенклатура = ТоварыВПути.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПоставщикиСрезПоследних.Номенклатура,
	МАКСИМУМ(ПоставщикиСрезПоследних.Период) КАК ДатаПоследнегоПоступления
ПОМЕСТИТЬ ПоследниеЗакупки
ИЗ
	РегистрСведений.Поставщики.СрезПоследних(
			,
			Номенклатура В
				(ВЫБРАТЬ
					Резервы.Номенклатура
				ИЗ
					Резервы)) КАК ПоставщикиСрезПоследних

СГРУППИРОВАТЬ ПО
	ПоставщикиСрезПоследних.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Закуп.Номенклатура,
	Пост.Поставщик
ПОМЕСТИТЬ ПоследнийПоставщик
ИЗ
	ПоследниеЗакупки КАК Закуп
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.Поставщики.СрезПоследних(
				,
				Номенклатура В
					(ВЫБРАТЬ
						Резервы.Номенклатура
					ИЗ
						Резервы)) КАК Пост
		ПО Закуп.Номенклатура = Пост.Номенклатура
			И Закуп.ДатаПоследнегоПоступления = Пост.Период
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	НоменклатураЗаменыСрезПоследних.Контрагент,
	НоменклатураЗаменыСрезПоследних.Номенклатура,
	НоменклатураЗаменыСрезПоследних.НоменклатураЗамена
ПОМЕСТИТЬ ТоварыЗамены
ИЗ
	РегистрСведений.НоменклатураЗамены.СрезПоследних(
			,
			Номенклатура В
					(ВЫБРАТЬ
						Резервы.Номенклатура
					ИЗ
						Резервы)
				И Контрагент В
					(ВЫБРАТЬ
						Резервы.Контрагент
					ИЗ
						Резервы)) КАК НоменклатураЗаменыСрезПоследних
ГДЕ
	НоменклатураЗаменыСрезПоследних.Использование = ИСТИНА
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Тов.Номенклатура,
	Тов.Контрагент,
	СУММА(ЕСТЬNULL(Ост.КоличествоОстаток, 0) - ЕСТЬNULL(Рез.КоличествоОстаток, 0)) КАК ОстатокЗаменыНаСкладе
ПОМЕСТИТЬ ТоварыЗаменыОстатки
ИЗ
	ТоварыЗамены КАК Тов
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ТоварыНаСкладах.Остатки(
				,
				Номенклатура В
					(ВЫБРАТЬ
						ТоварыЗамены.НоменклатураЗамена
					ИЗ
						ТоварыЗамены)) КАК Ост
		ПО Тов.НоменклатураЗамена = Ост.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ТоварыВРезерве.Остатки(
				,
				Номенклатура В
					(ВЫБРАТЬ
						ТоварыЗамены.НоменклатураЗамена
					ИЗ
						ТоварыЗамены)) КАК Рез
		ПО Тов.НоменклатураЗамена = Рез.Номенклатура

СГРУППИРОВАТЬ ПО
	Тов.Контрагент,
	Тов.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Разм.Заказ,
	Разм.Ответственный,
	Разм.ЗакупитьНедостающее,
	Разм.ДатаОтправкиВЗакупку,
	Разм.Номенклатура,
	Разм.Производитель,
	Разм.КатегорияТовара,
	Пост.Поставщик,
	Разм.Очередь,
	МАКСИМУМ(Разм.КоличествоОстаток) КАК КоличествоОстаток,
	Закуп.ЗаказПоставщику,
	СУММА(Закуп.КоличествоОстаток) КАК Заказано,
	СУММА(Тов.ОстатокЗаменыНаСкладе) КАК ОстатокЗаменыНаСкладе,
	СУММА(Разм.ОстатокВсего) КАК ОстатокВсего,
	СУММА(Разм.ОстатокНаЦСкладе) КАК ОстатокНаЦСкладе,
	СУММА(Разм.Резерв) КАК Резерв,
	СУММА(Разм.РезервНаЦСкладе) КАК РезервНаЦСкладе,
	СУММА(Разм.ВПути) КАК ВПути,
	Разм.МенеджерЗакупок,
	Закуп.ЗаказПоставщику.ДатаПоступления КАК ДатаПоступления,
	ВЫБОР
		КОГДА Разм.ЗакупитьНедостающее = ЛОЖЬ
			ТОГДА "Не требует заказа"
		ИНАЧЕ ВЫБОР
				КОГДА Разм.ОстатокНаЦСкладе - Разм.РезервНаЦСкладе &gt; 0
					ТОГДА "Есть на ЦС"
				ИНАЧЕ ВЫБОР
						КОГДА Закуп.ЗаказПоставщику &lt;&gt; ЗНАЧЕНИЕ(Документ.ЗАказПоставщику.ПустаяСсылка)
							ТОГДА ВЫБОР
									КОГДА Закуп.ЗаказПоставщику.ДатаПоступления &lt; &amp;ТекущаяДата
										ТОГДА "Просрочено"
									ИНАЧЕ "Заказано"
								КОНЕЦ
						ИНАЧЕ ВЫБОР
								КОГДА Разм.КоличествоОстаток &lt; Разм.ВПути + Закуп.КоличествоОстаток
									ТОГДА "Заказано"
								ИНАЧЕ "Заказать"
							КОНЕЦ
					КОНЕЦ
			КОНЕЦ
	КОНЕЦ КАК СтатусПотребности,
	Разм.Комментарий
ИЗ
	Резервы КАК Разм
		ЛЕВОЕ СОЕДИНЕНИЕ ПоследнийПоставщик КАК Пост
		ПО Разм.Номенклатура = Пост.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ЗаказыПоставщикам.Остатки(
				,
				Номенклатура В
					(ВЫБРАТЬ
						Резервы.Номенклатура
					ИЗ
						Резервы)) КАК Закуп
		ПО Разм.Номенклатура = Закуп.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ ТоварыЗаменыОстатки КАК Тов
		ПО Разм.Номенклатура = Тов.Номенклатура
			И Разм.Контрагент = Тов.Контрагент

СГРУППИРОВАТЬ ПО
	Разм.Заказ,
	Разм.Ответственный,
	Разм.ЗакупитьНедостающее,
	Разм.ДатаОтправкиВЗакупку,
	Разм.Номенклатура,
	Разм.Производитель,
	Разм.КатегорияТовара,
	Пост.Поставщик,
	Разм.Очередь,
	Закуп.ЗаказПоставщику,
	Разм.МенеджерЗакупок,
	Закуп.ЗаказПоставщику.ДатаПоступления,
	ВЫБОР
		КОГДА Разм.ЗакупитьНедостающее = ЛОЖЬ
			ТОГДА "Не требует заказа"
		ИНАЧЕ ВЫБОР
				КОГДА Разм.ОстатокНаЦСкладе - Разм.РезервНаЦСкладе &gt; 0
					ТОГДА "Есть на ЦС"
				ИНАЧЕ ВЫБОР
						КОГДА Закуп.ЗаказПоставщику &lt;&gt; ЗНАЧЕНИЕ(Документ.ЗАказПоставщику.ПустаяСсылка)
							ТОГДА ВЫБОР
									КОГДА Закуп.ЗаказПоставщику.ДатаПоступления &lt; &amp;ТекущаяДата
										ТОГДА "Просрочено"
									ИНАЧЕ "Заказано"
								КОНЕЦ
						ИНАЧЕ ВЫБОР
								КОГДА Разм.КоличествоОстаток &lt; Разм.ВПути + Закуп.КоличествоОстаток
									ТОГДА "Заказано"
								ИНАЧЕ "Заказать"
							КОНЕЦ
					КОНЕЦ
			КОНЕЦ
	КОНЕЦ,
	Разм.Комментарий</query>
