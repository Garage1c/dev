			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Спр.Ссылка 							КАК Номенклатура,
	Спр.Артикул 						КАК Артикул,
	ПОДСТРОКА(Спр.Комментарий, 1, 150) 	КАК Комментарий,
    Спр.Продажи81 						КАК Продажи81,
	ЕСТЬNULL(ПродажиОбороты.КоличествоОборот, 0) 			КАК Продано,
	ЕСТЬNULL(ТоварыНаСкладахОстатки.КоличествоОстаток, 0) 	КАК Остатки,
	ЕСТЬNULL(ТоварыВРезервеОстатки.КоличествоОстаток, 0)	КАК ВРезерве,
	ЕСТЬNULL(РазмещениеЗаказовОстатки.КоличествоОстаток, 0) КАК ЕстьПотребность,
	0	Поступило,
	0	ВПути,
	0	Заказано
    
ИЗ
	Справочник.Номенклатура КАК Спр

		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ТоварыВРезерве.Остатки(, {(Номенклатура.*) КАК Номенклатура}) КАК ТоварыВРезервеОстатки
		ПО Спр.Ссылка = ТоварыВРезервеОстатки.Номенклатура
	
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.Продажи.Обороты(, , , {(Номенклатура.*) КАК Номенклатура}) КАК ПродажиОбороты
		ПО Спр.Ссылка = ПродажиОбороты.Номенклатура
		
		ЛЕВОЕ СОЕДИНЕНИЕ
			РегистрНакопления.РазмещениеЗаказов.Остатки(,{(Номенклатура.*) КАК Номенклатура}) КАК РазмещениеЗаказовОстатки
		ПО Спр.Ссылка = РазмещениеЗаказовОстатки.Номенклатура
				
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ТоварыНаСкладах.Остатки(, {(Номенклатура.*) КАК Номенклатура}) КАК ТоварыНаСкладахОстатки
		ПО Спр.Ссылка = ТоварыНаСкладахОстатки.Номенклатура
		
		
{ГДЕ
	Спр.Ссылка.* КАК Номенклатура}</query>
