		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ПродажиОбороты.Номенклатура,
	ПродажиОбороты.КоличествоОборот КАК ПродажиШт,
	ПродажиОбороты.СуммаОборот КАК ПродажиСумма
ПОМЕСТИТЬ ПродажиПартнера
ИЗ
	РегистрНакопления.Продажи.Обороты(&amp;НачПериода, &amp;КонПериода, ,  {(Склад В (&amp;Склады))} {(Заказ.Партнер В (&amp;Партнеры))}) КАК ПродажиОбороты
ГДЕ
	ПродажиОбороты.СуммаОборот &gt; 0
	И ПродажиОбороты.КоличествоОборот &gt; 0

;

ВЫБРАТЬ
	ПродажиОбороты.Номенклатура,
	ПродажиОбороты.КоличествоОборот КАК ПродажиШт,
	ПродажиОбороты.СуммаОборот КАК ПродажиСумма,
	ЕСТЬNULL(ПродПарт.ПродажиШт, 0) КАК ПродажиШтПартнера,
	ЕСТЬNULL(ПродПарт.ПродажиСумма, 0) КАК ПродажиСуммаПартнера
ИЗ
	РегистрНакопления.Продажи.Обороты(&amp;НачПериода, &amp;КонПериода, , {(Склад В (&amp;Склады))}) КАК ПродажиОбороты
	ЛЕВОЕ СОЕДИНЕНИЕ
	ПродажиПартнера ПродПарт ПО ПродажиОбороты.Номенклатура = ПродПарт.Номенклатура
ГДЕ
	ПродажиОбороты.СуммаОборот &gt; 0
	И ПродажиОбороты.КоличествоОборот &gt; 0

</query>
