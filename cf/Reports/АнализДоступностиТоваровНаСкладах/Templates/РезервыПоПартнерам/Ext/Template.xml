		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Рез.Размещение 									Склад,
	Рез.ДокументРезерва.Дата						ДатаРезерва,
	Рез.ДокументРезерва,
	Рез.ДокументРезерва.Контрагент 					Контрагент,
	Рез.ДокументРезерва.Контрагент.ОсновнойМенеджер 	Менеджер,
	Рез.Номенклатура,
	Рез.Номенклатура.Артикул						Артикул,
	Рез.КоличествоОстаток Резерв,
	ЕСТЬNULL(Ост.КоличествоОстаток, 0) Остаток,
	ЕСТЬNULL(Ост.КоличествоОстаток, 0) - Рез.КоличествоОстаток СвободныйОстаток
ИЗ
	РегистрНакопления.ТоварыВРезерве.Остатки Рез
	
ЛЕВОЕ СОЕДИНЕНИЕ
	РегистрНакопления.ТоварыНаСкладах.Остатки Ост
ПО
	Рез.Номенклатура 	= Ост.Номенклатура И
	Рез.Размещение 		= Ост.Склад</query>
