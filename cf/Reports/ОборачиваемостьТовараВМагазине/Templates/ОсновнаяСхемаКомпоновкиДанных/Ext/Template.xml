				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Заказано по внутр. заказу</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЕСТЬNULL(Ост.Номенклатура, Рез.Номенклатура) Номенклатура,
	ЕСТЬNULL(Ост.КоличествоОстаток, 0) - ЕСТЬNULL(Рез.КоличествоОстаток, 0) Остаток,
	0 Продано, 0 ВПути, 0 Заказано
ИЗ
	РегистрНакопления.ТоварыНаСкладах.Остатки({&amp;ОстаткиНаДату},{Склад.* КАК Склад, Номенклатура В (ВЫБРАТЬ Номенклатура Из Справочник.НоменклатурныеГруппы.Состав ГДЕ Ссылка = &amp;НоменклатурнаяГруппа)}) Ост
	
ПОЛНОЕ СОЕДИНЕНИЕ
	РегистрНакопления.ТоварыВРезерве.Остатки({&amp;ОстаткиНаДату},{Размещение.* КАК Склад, Номенклатура В (ВЫБРАТЬ Номенклатура Из Справочник.НоменклатурныеГруппы.Состав ГДЕ Ссылка = &amp;НоменклатурнаяГруппа)}) Рез
ПО
	Ост.Номенклатура 	= Рез.Номенклатура И
	Ост.Склад			= Рез.Размещение
	
ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Номенклатура, 0,
	КоличествоОборот, 0, 0
ИЗ
	РегистрНакопления.Продажи.Обороты({&amp;ПериодНачало},{&amp;ПериодОкончание},,{Склад.* КАК Склад, Номенклатура В (ВЫБРАТЬ Номенклатура Из Справочник.НоменклатурныеГруппы.Состав ГДЕ Ссылка = &amp;НоменклатурнаяГруппа)})
	
ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Номенклатура, 0, 0,
	КоличествоОстаток, 0
ИЗ
	РегистрНакопления.ТоварыВПути.Остатки({&amp;ОстаткиНаДату},{СкладПолучатель.* КАК Склад, Номенклатура В (ВЫБРАТЬ Номенклатура Из Справочник.НоменклатурныеГруппы.Состав ГДЕ Ссылка = &amp;НоменклатурнаяГруппа)})
	
ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Номенклатура, 0, 0, 0,
	КоличествоОстаток
ИЗ
	РегистрНакопления.ВнутренниеЗаказы.Остатки({&amp;ОстаткиНаДату},{Заказчик.* КАК Склад, Номенклатура В (ВЫБРАТЬ Номенклатура Из Справочник.НоменклатурныеГруппы.Состав ГДЕ Ссылка = &amp;НоменклатурнаяГруппа)})</query>
