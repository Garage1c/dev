		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Спр.Ссылка									Номенклатура,
	Спр.Родитель 								НоменклатурнаяГруппа,
	ЕСТЬNULL(Прод.КоличествоОборот, 0) 			Количество,
	ЕСТЬNULL(Прод.СуммаОборот, 0)				Продажи,
	ЕСТЬNULL(Цен1.Цена,ЕСТЬNULL(Цен2.Цена,0)) 	ЦенаТипаЦен,
	ВЫБОР 
		КОГДА ЕСТЬNULL(Прод.КоличествоОборот, 0) &lt;&gt; 0 
		ТОГДА Прод.СуммаОборот / ЕСТЬNULL(Прод.КоличествоОборот, 0) 
		ИНАЧЕ 0 
	КОНЕЦ 										СредняяЦенаПродажи
	
ИЗ
	Справочник.Номенклатура Спр
	
ЛЕВОЕ СОЕДИНЕНИЕ
	РегистрНакопления.Продажи.Обороты() Прод
ПО
	Спр.Ссылка = Прод.Номенклатура
		
ЛЕВОЕ СОЕДИНЕНИЕ
	РегистрСведений.ЦеныНоменклатуры.СрезПоследних(,ТипЦен = &amp;ТипЦен1) Цен1
ПО
	Прод.Номенклатура = Цен1.Номенклатура
	
ЛЕВОЕ СОЕДИНЕНИЕ
	РегистрСведений.ЦеныНоменклатуры.СрезПоследних(,ТипЦен = &amp;ТипЦен2) Цен2
ПО
	Прод.Номенклатура = Цен2.Номенклатура
</query>
