		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Ост.Склад,
	Ост.Регистратор,
	Ост.Партия,
	Ост.ПериодДень КАК День,
	Ост.Номенклатура,
	Ост.КоличествоПриход,
	Ост.КоличествоРасход,
	Ост.КоличествоНачальныйОстаток,
	Ост.КоличествоКонечныйОстаток,
	Ост.СуммаПриход,
	Ост.СуммаРасход,
	Ост.СуммаНачальныйОстаток,
	Ост.СуммаКонечныйОстаток,
	ВЫБОР КОГДА Ост.КоличествоПриход = 0 ТОГДА 0 ИНАЧЕ Ост.СуммаПриход / Ост.КоличествоПриход КОНЕЦ СебестоимостьПриход,
	ВЫБОР КОГДА Ост.КоличествоРасход = 0 ТОГДА 0 ИНАЧЕ Ост.СуммаРасход / Ост.КоличествоРасход КОНЕЦ СебестоимостьРасход,
	ВЫБОР КОГДА Ост.КоличествоНачальныйОстаток = 0 ТОГДА 0 ИНАЧЕ Ост.СуммаНачальныйОстаток / Ост.КоличествоНачальныйОстаток КОНЕЦ СебестоимостьНачальныйОстаток,
	ВЫБОР КОГДА Ост.КоличествоКонечныйОстаток = 0 ТОГДА 0 ИНАЧЕ Ост.СуммаКонечныйОстаток / Ост.КоличествоКонечныйОстаток КОНЕЦ СебестоимостьКонечныйОстаток
//{	
//ВЫБРАТЬ
//	Ост.Регистратор КАК ДокДвижения,
//	Ост.Период		КАК Период
//}
ИЗ
	РегистрНакопления.ПартииТоваров.ОстаткиИОбороты(, , Авто, Движения, Номенклатура = &amp;Номенклатура) Ост
</query>
