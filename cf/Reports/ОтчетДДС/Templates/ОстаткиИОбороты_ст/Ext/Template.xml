		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДС.Регистратор КАК Документ,
	ДС.ПериодМесяц КАК МесяцОперации,
	ДС.ПериодДень КАК ДеньОперации,
	ВЫБОР
		КОГДА ДС.КассаСчет ССЫЛКА Справочник.Кассы
			ТОГДА "Касса"
		ИНАЧЕ "Банк"
	КОНЕЦ КАК Расчет,
	ДС.СтатьяДДС,
	ДС.ФизЛицоПартнер,
	ДС.КассаСчет,
	ДС.СуммаОборот КАК Сумма,
	ДС.СуммаУпрОборот КАК СуммаУпр,
	Остатки.СуммаОборот КАК ОстатокОборот,
	Остатки.СуммаПриход КАК ОстатокПриход,
	Остатки.СуммаРасход КАК ОстатокРасход,
	Остатки.СуммаКонечныйОстаток КАК ОстатокКонОст,
	Остатки.СуммаНачальныйОстаток КАК ОстатокНачОст,
	Ост.СуммаОстаток КАК НачОстатокКассы,
	ДС.ЦФУ
ИЗ
	РегистрНакопления.ОборотыДенежныхСредств.Обороты(, , Авто, ) КАК ДС
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ДенежныеСредства.ОстаткиИОбороты(, , Авто, , ) КАК Остатки
		ПО ДС.КассаСчет = Остатки.Касса
			И ДС.Регистратор = Остатки.Регистратор
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ДенежныеСредства.Остатки({(&amp;НачалоПериода)}, ) КАК Ост
		ПО ДС.КассаСчет = Ост.Касса</query>
