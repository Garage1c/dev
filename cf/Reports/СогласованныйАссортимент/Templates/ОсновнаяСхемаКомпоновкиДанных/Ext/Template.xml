		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ТоварыДляПередачиСрезПоследних.Период,
	ТоварыДляПередачиСрезПоследних.Номенклатура,
	ТоварыДляПередачиСрезПоследних.Контрагент
ПОМЕСТИТЬ Ассортимент
ИЗ
	РегистрСведений.ТоварыДляПередачи.СрезПоследних({(&amp;Период)}, {(Контрагент) КАК Контрагент, (Номенклатура В
			    (ВЫБРАТЬ
			        Справочник.НоменклатурныеГруппы.Состав.Номенклатура
			    ИЗ
			        Справочник.НоменклатурныеГруппы.Состав
			    ГДЕ
			        Справочник.НоменклатурныеГруппы.Состав.Ссылка = &amp;НоменклатурнаяГруппа))}) КАК ТоварыДляПередачиСрезПоследних
ГДЕ
	ТоварыДляПередачиСрезПоследних.Использовать = ИСТИНА
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	Ассортимент.Номенклатура
ПОМЕСТИТЬ Товары
ИЗ
	Ассортимент КАК Ассортимент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Тов.Период 							ДатаСогласования,
	Тов.Номенклатура,
	Тов.Номенклатура.Артикул 			Артикул,
	Тов.Контрагент,
	ЕСТЬNULL(Прод.СуммаОборот, 0) 		СуммаПродано,
	ЕСТЬNULL(Прод.КоличествоОборот, 0)	КоличествоПродано,
	ЕСТЬNULL(Ост.КоличествоОстаток, 0)  Количество,
	Цен.ТипЦен,
	ЕСТЬNULL(Цен.Цена, 0) * (ЕСТЬNULL(ТабВал.Курс, 1) * ЕСТЬNULL(ВалРуб.Кратность, 1)) / (ЕСТЬNULL(ТабВал.Кратность, 1) * ЕСТЬNULL(ВалРуб.Курс, 1)) КАК Цена,
	Впут.ДатаПоступления
ИЗ
	Ассортимент Тов
	ЛЕВОЕ СОЕДИНЕНИЕ 
		РегистрНакопления.ТоварыНаСкладах.Остатки({(&amp;Период)},	Номенклатура В (ВЫБРАТЬ Товары.Номенклатура	ИЗ Товары) {(Склад) КАК Склады}) КАК Ост
		ПО Тов.Номенклатура = Ост.Номенклатура
		
	ЛЕВОЕ СОЕДИНЕНИЕ 
		РегистрНакопления.ПродажиПоМВЗ.Обороты( ,{(&amp;Период)}, ,Номенклатура В (ВЫБРАТЬ Товары.Номенклатура	ИЗ Товары) {(Склад) КАК Склады}) Прод
		ПО Тов.Номенклатура = Прод.Номенклатура
		
	ЛЕВОЕ СОЕДИНЕНИЕ 
		РегистрСведений.ЦеныНоменклатуры.СрезПоследних({(&amp;Период), (&amp;ЦенаНаДату)}, Номенклатура В (ВЫБРАТЬ Товары.Номенклатура ИЗ Товары) {(ТипЦен) КАК ТипЦен}) КАК Цен
		ПО (Цен.Номенклатура = Тов.Номенклатура)
	
	ЛЕВОЕ СОЕДИНЕНИЕ 
		РегистрСведений.КурсыВалют.СрезПоследних({(&amp;Период), (&amp;КурсНаДату)}, ) КАК ТабВал
		ПО (Цен.Валюта = ТабВал.Валюта)
		
	ЛЕВОЕ СОЕДИНЕНИЕ 
		РегистрСведений.КурсыВалют.СрезПоследних({(&amp;Период), (&amp;КурсНаДату)}, Валюта = &amp;Валюта) КАК ВалРуб
		ПО (ИСТИНА)
	
	ЛЕВОЕ СОЕДИНЕНИЕ 
	(	ВЫБРАТЬ
			ТоварыПоставщиковВПутиОстатки.Номенклатура КАК Номенклатура,
			МИНИМУМ(ТоварыПоставщиковВПутиОстатки.Инвойс.ДатаПоступления) КАК ДатаПоступления
		ИЗ
			РегистрНакопления.ТоварыПоставщиковВПути.Остатки(, Номенклатура В (ВЫБРАТЬ Товары.Номенклатура ИЗ Товары)) КАК ТоварыПоставщиковВПутиОстатки
		
			СГРУППИРОВАТЬ ПО ТоварыПоставщиковВПутиОстатки.Номенклатура
	) КАК Впут
	ПО Тов.Номенклатура = Впут.Номенклатура

{ГДЕ
	Цен.Валюта = &amp;ВалютаУстановкиЦен}</query>
