		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ 
	НАЧАЛОПЕРИОДА(Сбор.Период, МЕСЯЦ) 	ПериодМесяц,
	КОНЕЦПЕРИОДА(Сбор.Период, МЕСЯЦ) 	ПериодМесяцК,
	НАЧАЛОПЕРИОДА(Сбор.Период, МЕСЯЦ) 	ПериодНеделя,
	Сбор.Период День,
	Сбор.Сборщик,
	ПОДСТРОКА(Сбор.Сборщик.Наименование, 1, 10) + ".." стрСборщик,
	Сбор.Заказ,
	Сбор.СборочныйЛист,
	Сбор.СборочныйЛист.Вес Вес,
	Сбор.СборочныйЛист.Объем Объем,
	Сбор.Заказ.Контрагент,
	Сбор.Номенклатура,
	Сбор.Упаковка,
	Сбор.Склад,
	Сбор.Ячейка,
	Сбор.КоличествоОборот Собрано,
	ЕСТЬNULL(Норм.ПозицийВДень, 0) Норма,
	Сбор.КоличествоОборот - ЕСТЬNULL(Норм.ПозицийВДень, 0) СверхН
	
ИЗ
	РегистрНакопления.СборщикЗаказа.Обороты(, , День, ) Сбор
	
ЛЕВОЕ СОЕДИНЕНИЕ
	(
		ВЫБРАТЬ Сбор.Сборщик, НАЧАЛОПЕРИОДА(Сбор.Период, ДЕНЬ) Период, МАКСИМУМ(Норм.Период) ПериодНормы
		ИЗ РегистрНакопления.СборщикЗаказа Сбор
	
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ	РегистрСведений.НормаВыработкиСборщиков Норм
		ПО	Сбор.Сборщик = Норм.Сборщик.ФизЛицо И
			НАЧАЛОПЕРИОДА(Сбор.Период, ДЕНЬ) &gt;= Норм.Период
		{
		ГДЕ
			Сбор.Сборщик.* 	КАК Сборщик,
			Сбор.Заказ.* 	КАК Заказ,
			Номенклатура.* 	КАК Номенклатура
		}
		СГРУППИРОВАТЬ ПО Сбор.Сборщик, НАЧАЛОПЕРИОДА(Сбор.Период, ДЕНЬ)
	) Дат
ПО 	
	Сбор.Сборщик 						= Дат.Сборщик И
	НАЧАЛОПЕРИОДА(Сбор.Период, ДЕНЬ)	= Дат.Период	
	
ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НормаВыработкиСборщиков Норм
ПО 	Сбор.Сборщик 	= Норм.Сборщик.ФизЛицо И
	Дат.ПериодНормы	= Норм.Период	</query>
