		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЗаказПоставщикуТовары.Ссылка,
	ЗаказПоставщикуТовары.Номенклатура,
	ЗаказПоставщикуТовары.Количество
ПОМЕСТИТЬ ТаблицаЗаказов
ИЗ
	Документ.ЗаказПоставщику.Товары КАК ЗаказПоставщикуТовары
ГДЕ
	ЗаказПоставщикуТовары.Ссылка.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаКонца
	И ЗаказПоставщикуТовары.Ссылка.Проведен = ИСТИНА
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТаблицаЗаказов.Ссылка,
	ТаблицаЗаказов.Количество,
	ТаблицаЗаказов.Номенклатура КАК Номенклатура,
	ВложенныйЗапрос.Регистратор,
	ВложенныйЗапрос.КоличествоНачальныйОстаток,
	ВложенныйЗапрос.КоличествоКонечныйОстаток,
	ЕСТЬNULL(ВложенныйЗапрос.КоличествоОборот, 0) КАК КоличествоОборот,
	ВложенныйЗапрос.КоличествоНачальныйОстаток1,
	ВложенныйЗапрос.КоличествоКонечныйОстаток1,
	ЕСТЬNULL(ВложенныйЗапрос.КоличествоОборот1, 0) КАК КоличествоОборот1
ИЗ
	ТаблицаЗаказов КАК ТаблицаЗаказов
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ЗаказыПоставщикамОстаткиИОбороты.Регистратор КАК Регистратор,
			ЗаказыПоставщикамОстаткиИОбороты.КоличествоНачальныйОстаток КАК КоличествоНачальныйОстаток,
			ЗаказыПоставщикамОстаткиИОбороты.КоличествоКонечныйОстаток КАК КоличествоКонечныйОстаток,
			ЗаказыПоставщикамОстаткиИОбороты.КоличествоОборот КАК КоличествоОборот,
			NULL КАК КоличествоНачальныйОстаток1,
			NULL КАК КоличествоКонечныйОстаток1,
			NULL КАК КоличествоОборот1,
			ЗаказыПоставщикамОстаткиИОбороты.Номенклатура КАК Номенклатура,
			ЗаказыПоставщикамОстаткиИОбороты.ЗаказПоставщику КАК ЗаказПоставщику
		ИЗ
			РегистрНакопления.ЗаказыПоставщикам.ОстаткиИОбороты(
					,
					,
					Авто,
					,
					ЗаказПоставщику В
						(ВЫБРАТЬ
							ТаблицаЗаказов.Ссылка
						ИЗ
							ТаблицаЗаказов)) КАК ЗаказыПоставщикамОстаткиИОбороты
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ
			ТоварыПоставщиковВПутиОстаткиИОбороты.Регистратор,
			NULL,
			NULL,
			NULL,
			ТоварыПоставщиковВПутиОстаткиИОбороты.КоличествоНачальныйОстаток,
			ТоварыПоставщиковВПутиОстаткиИОбороты.КоличествоКонечныйОстаток,
			ТоварыПоставщиковВПутиОстаткиИОбороты.КоличествоОборот,
			ТоварыПоставщиковВПутиОстаткиИОбороты.Номенклатура,
			ТоварыПоставщиковВПутиОстаткиИОбороты.ЗаказПоставщику
		ИЗ
			РегистрНакопления.ТоварыПоставщиковВПути.ОстаткиИОбороты(
					,
					,
					Авто,
					,
					ЗаказПоставщику В
						(ВЫБРАТЬ
							ТаблицаЗаказов.Ссылка
						ИЗ
							ТаблицаЗаказов)) КАК ТоварыПоставщиковВПутиОстаткиИОбороты) КАК ВложенныйЗапрос
		ПО ТаблицаЗаказов.Номенклатура = ВложенныйЗапрос.Номенклатура
			И ТаблицаЗаказов.Ссылка = ВложенныйЗапрос.ЗаказПоставщику</query>
