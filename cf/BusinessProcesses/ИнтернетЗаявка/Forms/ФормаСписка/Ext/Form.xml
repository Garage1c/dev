	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	БП.Ссылка,
	БП.Заказ.Номер КАК НомерЗаказа,
	БП.Заказ.Дата КАК ДатаЗаказа,
	БП.Заказ,
	БП.Заказ.Контрагент КАК Контрагент,
	БП.Заказ.Склад КАК Склад,
	БП.Заказ.ТипЦен КАК ТипЦен,
	БП.Заказ.Инициатор КАК Инициатор,
	БП.Заказ.АдресДоставки КАК АдресДоставки,
	ЕСТЬNULL(Рег.СуммаПриход, БП.Заказ.Сумма) КАК Сумма,
	ВЫБОР
		КОГДА ЕСТЬNULL(Рег.СуммаПриход, БП.Заказ.Сумма) = 0
			ТОГДА 0
		ИНАЧЕ ВЫРАЗИТЬ(100 * Опл.суммаОборот / ЕСТЬNULL(Рег.СуммаПриход, БП.Заказ.Сумма) КАК ЧИСЛО(10, 2))
	КОНЕЦ КАК ПроцентОплаты,
	ВЫБОР
		КОГДА ЕСТЬNULL(Рег.СуммаПриход, 0) = 0
			ТОГДА 0
		КОГДА ЕСТЬNULL(Опл.суммаОборот, 0) = 0
			ТОГДА 1
		КОГДА ЕСТЬNULL(Опл.суммаОборот, 0) = ЕСТЬNULL(Рег.СуммаПриход, БП.Заказ.Сумма)
			ТОГДА 3
		ИНАЧЕ 2
	КОНЕЦ КАК КартинкаОплаты,
	БП.Заказ.ПользовательИнтернет КАК ПользовательИнтернет,
	БП.Заказ.СайтИсточник КАК СайтИсточник,
	ПОДСТРОКА(БП.Комментарий, 1, 255) КАК Комментарий,
	Сос.Состояние,
	Отгрузка.ТС,
	Жур.ПричинаОтказа,
	БП.Заказ.МестоположениеСтр КАК Местоположение,
	ВЫБОР
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.ВОчередиНаСклад)
			ТОГДА 1
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.ВОчередиНаСкладПовторно)
			ТОГДА 17
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.ВРаботе)
			ТОГДА 2
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.ОжидаетПеремещения)
			ТОГДА 3
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.ОжиданиеОплаты)
			ТОГДА 4
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.ОжиданиеПодтверждения)
			ТОГДА 5
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.Отгружен)
			ТОГДА 6
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.Открыт)
			ТОГДА 7
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.Отменен)
			ТОГДА 8
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.Перемещается)
			ТОГДА 9
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.Собирается)
			ТОГДА 10
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.Собран)
			ТОГДА 11
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.СобранЧастично)
			ТОГДА 16
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.Черновик)
			ТОГДА 12
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.ОжиданиеОтвета)
			ТОГДА 13
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.ВОчередиНаСборку)
			ТОГДА 15
		КОГДА Сос.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияЗаказа.Обработан)
			ТОГДА 7
	КОНЕЦ КАК КартинкаСостояния,
	ВЫБОР
		КОГДА (ВЫРАЗИТЬ(БП.Заказ.Admitad_uid КАК СТРОКА(100))) = ""
			ТОГДА ЛОЖЬ
		ИНАЧЕ ИСТИНА
	КОНЕЦ КАК Admitad_uid
ИЗ
	БизнесПроцесс.ИнтернетЗаявка КАК БП
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СостоянияЗаказовТекущее КАК Сос
		ПО БП.Заказ = Сос.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ИнтернетЗаказПокупателя.Обороты(, , , НЕ ИнтернетЗаказ ЕСТЬ NULL ) КАК Рег
		ПО БП.Заказ = Рег.ИнтернетЗаказ
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ДолгиПоОтгрузкам.Заказ КАК Заказ,
			СУММА(ДолгиПоОтгрузкам.Сумма) КАК суммаОборот
		ИЗ
			РегистрНакопления.ДолгиПоОтгрузкам КАК ДолгиПоОтгрузкам
		ГДЕ
			ДолгиПоОтгрузкам.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)
		
		СГРУППИРОВАТЬ ПО
			ДолгиПоОтгрузкам.Заказ) КАК Опл
		ПО БП.Заказ = Опл.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ЗаказыЖурнал КАК Жур
		ПО БП.Заказ = Жур.Заказ
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			НарядНаЗагрузкуТабЗаказов.Ссылка.ТранспортноеСредство КАК ТС,
			ВЫРАЗИТЬ(НарядНаЗагрузкуТабЗаказов.Заказ КАК Документ.ЗаказПокупателя) КАК Заказ
		ИЗ
			Документ.НарядНаЗагрузку.ТабЗаказов КАК НарядНаЗагрузкуТабЗаказов
		ГДЕ
			НЕ НарядНаЗагрузкуТабЗаказов.Ссылка.ПометкаУдаления
			И НЕ НарядНаЗагрузкуТабЗаказов.Ссылка.Проведен) КАК Отгрузка
		ПО (Отгрузка.Заказ = БП.Заказ)</QueryText>
